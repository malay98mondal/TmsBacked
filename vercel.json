{
    "version": 2,
    "builds": [
      {
        "src": "dist/app.js",  
        "use": "@vercel/node"
      },
      {
        "src": "client-dist/**/*", 
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/app.js"  
      }
    ]
  }
  